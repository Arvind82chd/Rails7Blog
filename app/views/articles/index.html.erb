<h1>All Articles:</h1>
<ul>
    <% @articles.each do |article| %>
        <a href="/articles/<%=article.id%>" >
        <strong><%= article.title %></strong> </a>
        <p><%= article.content %></p>
        <%= link_to "Show", article_path(article.id) %>
        </br>
        </br>
    <% end %>
</ul>
