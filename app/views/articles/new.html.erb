<h1> New Article: </h1>
<div>
    <%= form_with(model: @article, url: articles_new_path) do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title %>
        </br>
        </br>
        <%= form.label :content %>
        <%= form.text_area :content %>
        </br>
        </br>
        <%= form.submit %>
    </div>
    <% end %>
</div>